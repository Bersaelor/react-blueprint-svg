{
  "name": "react-blueprint-svg",
  "version": "0.2.0",
  "description": "Pannable, zoomable SVG with menu of displaying options inspired by maker.js.",
  "author": "Bersaelor",
  "license": "MIT",
  "repository": "Bersaelor/react-blueprint-svg",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "react-from-dom": "^0.3.1",
    "react-i18next": "^11.3.1"
  },
  "peerDependencies": {
    "makerjs": "^0.17.0",
    "prop-types": "^15.5.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-url": "^4.0.1",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "makerjs": "^0.17.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.5",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "svg",
    "playground",
    "scalable",
    "vector",
    "makerjs",
    "maker",
    "cnc"
  ]
}
